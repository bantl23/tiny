/if/         { return IF }
/then/       { return THEN }
/else/       { return ELSE }
/end/        { return END }
/repeat/     { return REPEAT }
/until/      { return UNTIL }
/read/       { return READ }
/write/      { return WRITE }
/:=/         { return ASSIGN }
/=/          { return EQ }
/</          { return LT }
/\+/         { return PLUS }
/-/          { return MINUS }
/\*/         { return TIMES }
/\//         { return OVER }
/\(/         { return LPAREN }
/\)/         { return RPAREN }
/;/          { return SEMI }
/[0-9]+/     { return NUM }
/[a-zA-Z]+/  { return ID }
/[ \t\r\n]+/ { /* white space */ }
/{[^\{\}]*}/ { /* comments */ }
/./          { return ERROR }
